{
  "name": "senioriteract",
  "version": "0.1.0",
  "private": true,
  "description": "Aplicación web con HTML, CSS y JavaScript puro para interacción con personas mayores",
  "scripts": {
    "dev": "live-server --port=3000 --open=/index.html",
    "build": "tailwindcss -i ./src/style.css -o ./dist/style.css --minify",
    "watch": "tailwindcss -i ./src/style.css -o ./dist/style.css --watch",
    "serve": "live-server --port=3000 --open=/index.html --no-browser"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.4"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "live-server": "^1.2.2",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5"
  },
  "keywords": [
    "html",
    "css",
    "javascript",
    "tailwindcss",
    "senior",
    "interaccion",
    "accesibilidad"
  ],
  "author": "Tu Nombre",
  "license": "MIT"
}
