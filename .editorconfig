# EditorConfig para mantener consistencia en el c√≥digo
# https://editorconfig.org/

root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = space
indent_size = 2

[*.{html,css,js}]
indent_size = 2

[*.md]
trim_trailing_whitespace = false

[*.py]
indent_size = 4

[*.json]
indent_size = 2
